QPushButton {
    background-color: #4CAF50;
    color: white;
    border: 2px solid #4CAF50;
    border-radius: 5px;
    font: bold 14px;
    padding: 10px;
    width: 10em;
    height: 10em;
}

QPushButton:pressed {
    background-color: #2E7031;
    border: 2px solid #2E7031;
}

QPushButton:hover:!pressed {
    background-color: #74AD6F;
}
