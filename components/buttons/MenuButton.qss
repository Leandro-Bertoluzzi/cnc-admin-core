QPushButton {
    background-color: #4CAF50;
    color: white;
    border: 2px solid #eeeeee;
    border-radius: 5px;
    font: bold 18px;
    margin: auto;
    padding: 10px;
    width: 15em;
    height: 15em;
}

QPushButton:pressed {
    background-color: #2E7031;
    border: 2px solid #2E7031;
}

QPushButton:hover:!pressed {
    background-color: #74AD6F;
}
